<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane stylesheets="@styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
   <children>
      <VBox layoutX="6.0" layoutY="8.0" prefHeight="595.0" prefWidth="846.0" styleClass="borderColor" stylesheets="@styles.css">
         <children>
            <Label fx:id="title" alignment="CENTER" prefHeight="77.0" prefWidth="1197.0" styleClass="backgroundColor" stylesheets="@styles.css" text="plagiarism Score" textFill="WHITE">
               <font>
                  <Font size="37.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="548.0" prefWidth="852.0" spacing="10.0">
               <children>
                  <VBox prefHeight="534.0" prefWidth="479.0">
                     <children>
                        <TextField fx:id="input" prefHeight="30.0" prefWidth="284.0" promptText="Input  Text">
                           <VBox.margin>
                              <Insets bottom="20.0" left="5.0" right="5.0" />
                           </VBox.margin>
                        </TextField>
                        <Button fx:id="goButton" alignment="CENTER" mnemonicParsing="false" onAction="#goAction" styleClass="buttonStyle" stylesheets="@styles.css" text="GO" textFill="WHITE">
                           <VBox.margin>
                              <Insets bottom="5.0" left="130.0" right="5.0" top="120.0" />
                           </VBox.margin>
                           <font>
                              <Font name="System Bold Italic" size="33.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
                  <VBox prefHeight="545.0" prefWidth="326.0">
                     <children>
                        <Label fx:id="output" alignment="CENTER" prefHeight="74.0" prefWidth="423.0" styleClass="outputStyle" stylesheets="@styles.css">
                           <font>
                              <Font name="System Bold Italic" size="34.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" top="2.0" />
   </padding>
   <styleClass>
      <String fx:value="whiteBackgroundColor" />
      <String fx:value="borderColor" />
   </styleClass>
</AnchorPane>
